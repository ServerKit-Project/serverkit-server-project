generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./default.db"
}

model FileInfo {
  id           String   @id @default(uuid())
  size         Int
  filename     String
  mimetype     String
  filePath     String?
  originalname String
  createdAt    DateTime @default(now())
}